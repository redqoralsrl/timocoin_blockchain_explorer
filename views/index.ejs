<!DOCTYPE html>
<html lang="ko">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Timocoin API</title>
    <link rel="stylesheet" href="/styles/index.css"/>
    <link rel="shortcut icon" href="/images/timo.ico" type="image/x-icon">
    <link rel="icon" href="/images/timo.ico" type="image/x-icon">
</head>
<body>
    <%- include('title') %>
    <div class="container">
        <div class="intro">
            <img src="/images/timo.png">
            Timocoin Explorer
        </div>
        <ul>
            <% if(userId != undefined) { %>
            <li><a href="/signin/logout">logout</a></li>
            <% } else { %>
            <li><a href="/signup">signup</a></li>
            <li><a href="/signin">signin</a></li>
            <% } %>
            <li><a href="/sendfrom">sendfrom</a></li>
            <li><a href="/getblockchaininfo">getblockchaininfo</a></li>
            <li><a href="/getnetworkinfo">getnetworkinfo</a></li>
            <li><a href="/getblockhash">getblockhash</a></li>
            <li><a href="/getmininginfo">getmininginfo</a></li>
            <li><a href="/getwalletinfo">getwalletinfo</a></li>
            <li><a href="/listwallets">listwallets</a></li>
            <li><a href="/listtransactions">listtransactions</a></li>
        </ul>
    </div>
</body>
</html>